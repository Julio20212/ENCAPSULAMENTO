package ObjetosConta;

import ContaBanca.EnderecoOp1;

public class ObjetosEnderecoOp1 {
	
	public static void main(String[] args) {
		
        EnderecoOp1 endereco = new EnderecoOp1(1, "Rua Exemplo", 123, "Apto 45", "Centro", "Cidade Exemplo", "12345-678", "SP");

        
        System.out.println("Endereco:");
        System.out.println("ID: " + endereco.getId());
        System.out.println("Logradouro: " + endereco.getLogradouro());
        System.out.println("NÃºmero: " + endereco.getNumero());
        System.out.println("Complemento: " + endereco.getComplemento());
        System.out.println("Bairro: " + endereco.getBairro());
        System.out.println("Cidade: " + endereco.getCidade());
        System.out.println("CEP: " + endereco.getCep());
        System.out.println("UF: " + endereco.getUf());
    }
}
